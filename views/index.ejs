<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="renderer" content="webkit">
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <title>校园音乐会管理系统</title>

    <!--[if lt IE 9]>
    <meta http-equiv="refresh" content="0;ie.html" />
    <![endif]-->

    <link rel="shortcut icon" href="/images/favicon.ico">
    <link href="/stylesheets/bootstrap.min14ed.css?v=3.3.6" rel="stylesheet">
    <link href="/stylesheets/font-awesome.min93e3.css?v=4.4.0" rel="stylesheet">
    <link href="/stylesheets/animate.min.css" rel="stylesheet">
    <link href="/stylesheets/style.min862f.css?v=4.1.0" rel="stylesheet">
    <link href="/stylesheets/plugins/dataTables/dataTables.bootstrap.css" rel="stylesheet">
</head>

<body class="fixed-sidebar full-height-layout gray-bg" style="overflow:hidden">
    <div id="wrapper">
        <!--左侧导航开始-->
        <nav class="navbar-default navbar-static-side" role="navigation">
            <div class="nav-close"><i class="fa fa-times-circle"></i>
            </div>
            <div class="sidebar-collapse">
                <ul class="nav" id="side-menu">
                    <li>
                        <a href="/">
                            <i class="fa fa-home"></i>
                            <span class="nav-label">主页</span>
                        </a>
                    </li>
                    <li>
                        <a href="/category">
                            <i class="fa fa fa-columns"></i>
                            <span class="nav-label">演奏类别维护</span>
                        </a>
                    </li>
                    <li>
                        <a href="/site">
                            <i class="fa fa fa-bar-chart-o"></i>
                            <span class="nav-label">场地信息维护</span>
                        </a>
                    </li>
                </ul>
            </div>
        </nav>
        <!--左侧导航结束-->
        <!--右侧部分开始-->
        <div id="page-wrapper" class="gray-bg dashbard-1">
            <div class="row content-tabs">
                <a href="/add" class="roll-nav roll-left" style="width:70px"><i class="fa fa-plus"></i>&nbsp;&nbsp;添加
                </a>
                <a id="edit" href="/updateDetails" class="roll-nav roll-left" style="width:70px;margin-left: 70px;"><i class="fa fa-pencil"></i>&nbsp;&nbsp;修改
                </a>
                <a id="del" href="/delDetails" class="roll-nav roll-left" style="width:70px;margin-left: 140px;"><i class="fa fa-minus"></i>&nbsp;&nbsp;删除
                </a>
            </div>
            <div class="row J_mainContent" id="content-main">
                <div class="ibox-content">
                    <table class="table table-striped table-bordered table-hover dataTables-example">
                        <thead>
                            <tr>
                                <th>学生姓名</th>
                                <th>演奏曲目</th>
                                <th>演奏形式</th>
                                <th>演出场地</th>
                                <th>演出时间</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% if(posts){ %>
                            <% posts.forEach(function(post,index){ %>
								<tr class="gradeX">
	                                <td title="<%- post.id %>"><%- post.student %></td>
	                                <td><%- post.track %></td>
	                                <td><%- post.category %></td>
	                                <td class="center"><%- post.site %></td>
	                                <td class="center"><%- post.time %></td>
	                            </tr>
							<% })} %>
                        </tbody>
                    </table>

                </div>
            </div>
        </div>
        <!--右侧部分结束-->
    </div>
    <script src="/javascripts/jquery.min.js?v=2.1.4"></script>
    <script src="/javascripts/bootstrap.min.js?v=3.3.6"></script>
    <script src="/javascripts/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="/javascripts/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="/javascripts/plugins/layer/layer.min.js"></script>
    <script src="/javascripts/hplus.min.js?v=4.1.0"></script>
    <script type="text/javascript" src="/javascripts/contabs.min.js"></script>
    <script src="/javascripts/plugins/pace/pace.min.js"></script>

    <script src="/javascripts/plugins/jeditable/jquery.jeditable.js"></script>
    <script src="/javascripts/plugins/dataTables/jquery.dataTables.js"></script>
    <script src="/javascripts/plugins/dataTables/dataTables.bootstrap.js"></script>
    <script src="/javascripts/content.min.js?v=1.0.0"></script>
    <script>
        $(document).ready(function(){
            $(".dataTables-example").dataTable();
            var oTable=$("#editable").dataTable();
            // oTable.$("td").editable("http://www.zi-han.net/theme/example_ajax.php",{"callback":function(sValue,y){
            //         var aPos=oTable.fnGetPosition(this);
            //         oTable.fnUpdate(sValue,aPos[0],aPos[1])
            //     },"submitdata":function(value,settings){
            //         return{"row_id":this.parentNode.getAttribute("id"),"column":oTable.fnGetPosition(this)[2]}
            //     },"width":"90%","height":"100%"
            // });
            $(".gradeX").click(function(){
                var id = $(this).children("td")[0].title;
                $("#edit").attr("href","/updateDetails/"+id);
                $("#del").attr("href","/delDetails/"+id);
            });
            $(".gohome").remove();
        });
    </script>
    
</body>


<!--  / -->
</html>
